# Server Configuration
NODE_ENV=development
PORT=5000
API_PREFIX=/api

# Database
DATABASE_URL="postgresql://postgres:password@localhost:5432/fastgpt_platform?schema=public"

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_EXPIRES_IN=7d

# Session Configuration
SESSION_SECRET=your-super-secret-session-key-change-this-in-production

# OAuth2 - Google
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
# GOOGLE_CALLBACK_URL will default to ${SITE_URL}/api/auth/oauth2/google/callback if not set

# OAuth2 - GitHub
GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret
# GITHUB_CALLBACK_URL will default to ${SITE_URL}/api/auth/oauth2/github/callback if not set

# CORS (comma-separated list of allowed origins)
# If not set, will use FRONTEND_URL value(s)
CORS_ORIGIN=http://localhost:3000

# Site URL (without trailing slash)
SITE_URL=http://localhost:5000

# Frontend URL (comma-separated list for multiple frontend URLs)
# The first URL will be used as the primary frontend URL
# All URLs will be added to CORS_ORIGIN automatically
FRONTEND_URL=http://localhost:3000

# DES Encryption Default (can be overridden in admin settings)
DEFAULT_DES_KEY=12345678
DEFAULT_DES_IV=12345678

# SSO Token Validity (minutes)
SSO_TOKEN_VALIDITY=5

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Admin Default Credentials (for initial setup)
ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD=Admin@123456
